{
	"manifest_version": 3,
	"name": "Cirrus Maximus",
	"description": "Scrape and extract data from the Cirrus online assessment platform.",
	"version": "1.0",
	"permissions": [
		"scripting",
		"tabs"
	],
	"host_permissions": [
		"https://*.cirrusplatform.com/*"
	],
	"action": {
		"default_title": "Cirrus Web Scraper",
		"default_popup": "popup.html",
		"default_icon": "images/icon-32.png"
	},
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	}
}