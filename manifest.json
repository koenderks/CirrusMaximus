{
	"manifest_version": 3,
	"name": "Cirrus Assessment Matrix",
	"description": "Extension to aggregate Cirrus exams into an assessment matrix.",
	"version": "1.0",
	"permissions": [
		"scripting"
	],
	"host_permissions": [
		"https://*.cirrusplatform.com/*"
	],
	"action": {
		"default_title": "Export assessment matrix",
		"default_popup": "popup.html",
		"default_icon": "images/icon-32.png"
	},
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	}
}